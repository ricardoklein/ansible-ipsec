swanctl {
  load = pem pkcs1 revocation constraints pubkey openssl random aes sha1 sha2
}

charon-systemd {
  load = random nonce openssl pem pkcs1 revocation curl kernel-netlink socket-default updown vici aes sha1 sha2
  install_virtual_ip_on = {{ install_virtual_ip_on }}
  install_virtual_ip = yes
  install_routes = yes 
  journal {
    default = 2
    ike = 2
    knl = 3
  }
}

